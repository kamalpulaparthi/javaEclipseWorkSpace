package practiceWordTabularquestionsOnNoSysArrayString;

public class Array90Rotation {

	public static void main(String[] args) {
		char [][]arr= {{'a','b','c'},
				      {'d','e','f'},
				      {'g','h','i'}};
		
		
		char [][]brr=new char[3][3];
		
		for(int i=0;i<3;i++)
		{
			for(int j=0;j<3;j++)
			{
				brr[j][3-i-1]=arr[i][j];
			}
			
		}
		
		
		for(int i=0;i<3;i++)
		{
			for(int j=0;j<3;j++)
			{
				System.out.println(brr[i][j]);
			}
		}
	}

}
