package strings;
import java.lang.*;

import java.util.*;
public class SpaceHiphen {

	public static void main(String[] args) {
		String str="i need hiphens in place of space";// TODO Auto-generated method stub
		for(int i=0;i<str.length();i++)
		{
			if(str.equals(" "))
			{
				str="-";
			}
		}
		
	}

}
